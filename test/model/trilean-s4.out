max expr size  =    4
  |- on ineqs  =    3
  |- on conds  =    3
max  #-tests   =  500
min  #-tests   =   25  (to consider p ==> q true)
max  #-vars    =    2  (for inequational and conditional laws)

_ :: Tril
M :: Tril
F :: Tril
T :: Tril
neg :: Tril -> Tril
(&&&) :: Tril -> Tril -> Tril
(|||) :: Tril -> Tril -> Tril

    p &&& p == p
    p ||| p == p
neg (neg p) == p
    p &&& T == p
    p ||| F == p
    p &&& M == M
    p ||| M == M
    p &&& q == q &&& p
    p ||| q == q ||| p
p &&& neg p == p &&& F
neg p &&& F == p &&& F
    p ||| T == neg (p &&& F)
p ||| neg p == neg (p &&& F)

      p <= T
      M <= p
p &&& q <= p
      p <= p ||| T
p &&& q <= p ||| q
p ||| q <= p ||| T

