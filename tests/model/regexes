max expr size  =    4
  |- on ineqs  =    3
  |- on conds  =    3
max  #-tests   =   30
max  #-vars    =    2  (for inequational and conditional laws)

_ :: RE Symbol  (holes: RE Symbol)
_ :: Bool  (holes: Bool)
Empty :: RE Symbol
None :: RE Symbol
Star :: RE Symbol -> RE Symbol
(:+) :: RE Symbol -> RE Symbol -> RE Symbol
(:.) :: RE Symbol -> RE Symbol -> RE Symbol
(<=) :: RE Symbol -> RE Symbol -> Bool
(==) :: RE Symbol -> RE Symbol -> Bool
False :: Bool
True :: Bool

           r :+ r == r
    Star (Star r) == Star r
        r :+ None == r
       r :. Empty == r
        r :. None == None
           r :+ s == s :+ r
           r :. s == s :. r
      r :+ Star r == Star r
Star (r :+ Empty) == Star r
  Empty :+ Star r == Star r
      (r == None) == (r <= None)
      (None <= r) == True
    (r <= Star r) == True
    (r == Star r) == (Star r <= r)
(Empty <= Star r) == True
(Empty == Star r) == (r <= Empty)
(Star r <= Empty) == (r <= Empty)
 (Star r <= None) == False
    (r <= Star s) == (r <= Star t)

      None <=  r
         r <=  Star r
     Empty <=  Star r
         r <=  r :+ s
     Empty <=  r :+ s
    r :. r <=  Star r
r :+ Empty <=  Star r
    r :. s <=  r :+ s
r :+ Empty <=  r :+ s
    r <= s ==> r <= Empty
    r <= s ==> Empty <= s
    s <= r ==> r <= Empty
    r == s ==> r == Empty
 r <= None ==> r <= s
    s <= r ==> r == Empty
 r <= None ==> s <= Empty
 r <= None ==> s == Empty

