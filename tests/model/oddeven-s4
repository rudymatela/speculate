max expr size  =    4
  |- on ineqs  =    3
  |- on conds  =    3
max  #-tests   =  500
min  #-tests   =   25  (to consider p ==> q true)
max  #-vars    =    2  (for inequational and conditional laws)

_ :: Int  (holes: Int)
_ :: Bool  (holes: Bool)
0 :: Int
1 :: Int
2 :: Int
False :: Bool
True :: Bool
odd :: Int -> Bool
even :: Int -> Bool
mod :: Int -> Int -> Int
(==) :: Int -> Int -> Bool
(==) :: Bool -> Bool -> Bool

        x `mod` 1 == 0
         (x == x) == True
         (p == p) == True
      (p == True) == p
         (x == y) == (y == x)
 even (x `mod` 2) == even x
  odd (x `mod` 2) == odd x
(False == even x) == odd x
 (False == odd x) == even x

x `mod` 2 <=  2
        0 <=  x `mod` 2
x `mod` 2 <=  1
        p ==> True
    False ==> p
   x == 0 ==> even x
   x == 1 ==> odd x
   x == 2 ==> even x

even x ==> x `mod` 2 == 0
 odd x ==> x `mod` x == 0
 odd x ==> 0 `mod` x == 0
 odd x ==> x `mod` 2 == 1

