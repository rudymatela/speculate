_ :: [Int]  (holes: [Int])
_ :: Int  (holes: Int)
0 :: Int
[] :: [Int]
(:) :: Int -> [Int] -> [Int]
(++) :: [Int] -> [Int] -> [Int]
length :: [Int] -> Int

        length [] == 0
         xs ++ [] == xs
         [] ++ xs == xs
length (xs ++ ys) == length (ys ++ xs)
    length (x:xs) == length (y:xs)

[] <= xs
 0 <= length xs

