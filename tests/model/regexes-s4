max expr size  =    4
  |- on ineqs  =    3
  |- on conds  =    3
max  #-tests   =   30
max  #-vars    =    2  (for inequational and conditional laws)

_ :: RE Symbol  (holes: RE Symbol)
Empty :: RE Symbol
None :: RE Symbol
Star :: RE Symbol -> RE Symbol
(:+) :: RE Symbol -> RE Symbol -> RE Symbol
(:.) :: RE Symbol -> RE Symbol -> RE Symbol

    Star r == Empty
    r :+ r == r
r :+ Empty == Empty
 r :+ None == r
r :. Empty == r
 r :. None == None
    r :+ s == s :+ r
    r :. s == s :. r

  None <= r
     r <= r :+ s
r :. s <= s
r :. r <= r :+ s

