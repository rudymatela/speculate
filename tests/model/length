_ :: [Int]  (holes: [Int])
_ :: Int  (holes: Int)
0 :: Int
[] :: [Int]
(:) :: Int -> [Int] -> [Int]
(++) :: [Int] -> [Int] -> [Int]
length :: [Int] -> Int

         xs ++ [] == xs
         [] ++ xs == xs
             x:xs == [x] ++ xs
 (xs ++ ys) ++ zs == xs ++ (ys ++ zs)
        length [] == 0
length (xs ++ ys) == length (ys ++ xs)
    length (x:xs) == length (y:xs)

[] <= xs
 0 <= length xs
xs <= xs ++ ys

