max expr size  =    4
max  #-tests   =  500
min  #-tests   =   25  (to consider p ==> q true)
max  #-vars    =    2  (for inequational and conditional laws)

_ :: Bool  (holes: Bool)
False :: Bool
True :: Bool
not :: Bool -> Bool
(&&) :: Bool -> Bool -> Bool
(||) :: Bool -> Bool -> Bool
(==) :: Bool -> Bool -> Bool

    (p && p) == p
    (p == p) == True
    (p || p) == p
 not (not p) == p
(p && False) == False
 (p && True) == p
(p == False) == not p
 (p == True) == p
(p || False) == p
 (p || True) == True
    (p && q) == (q && p)
    (p || q) == (q || p)
(p == not q) == not (p == q)
(p && not p) == False
(p || not p) == True

     p ==> True
 False ==> p
     p ==> p || q
p && q ==> p
p && q ==> p == q


