fromHSL :: Maybe (Ratio Integer) -> Ratio Integer -> Ratio Integer -> Color
mix :: Color -> Color -> Color
Just :: Ratio Integer -> Maybe (Ratio Integer)
0 % 1 :: Ratio Integer
1 % 1 :: Ratio Integer
1 % 2 :: Ratio Integer

