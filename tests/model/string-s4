_ :: [Char]  (holes: [Char])
_ :: [[Char]]  (holes: [[Char]])
"" :: [Char]
" " :: [Char]
"\n" :: [Char]
lines :: [Char] -> [[Char]]
words :: [Char] -> [[Char]]
unlines :: [[Char]] -> [Char]
unwords :: [[Char]] -> [Char]
(++) :: [Char] -> [Char] -> [Char]

                     cs ++ "" == cs
                     "" ++ cs == cs
unlines (lines (unlines css)) == unlines css
          words (unwords css) == words (unlines css)
   lines (unlines (lines cs)) == lines cs
   words (unlines (words cs)) == words cs
   lines (unlines (words cs)) == words cs
   words (unlines (lines cs)) == words cs
            words (cs ++ " ") == words cs
           words (cs ++ "\n") == words cs
            words (" " ++ cs) == words cs
           words ("\n" ++ cs) == words cs

      "" <= cs
lines "" <= css

