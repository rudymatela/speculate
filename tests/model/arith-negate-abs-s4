max expr size  =    4
  |- on ineqs  =    3
  |- on conds  =    3
max  #-tests   =  500
min  #-tests   =   25  (to consider p ==> q true)
max  #-vars    =    2  (for inequational and conditional laws)

_ :: Int  (holes: Int)
0 :: Int
1 :: Int
id :: Int -> Int
abs :: Int -> Int
negate :: Int -> Int
(+) :: Int -> Int -> Int
(*) :: Int -> Int -> Int

             id x == x
      abs (abs x) == abs x
negate (negate x) == x
   abs (negate x) == abs x
            x * 0 == 0
            x * 1 == x
            x + 0 == x
            x * y == y * x
            x + y == y + x
     x * negate y == negate (x * y)
      abs (x * x) == x * x
     x + negate x == 0

             x <= abs x
             0 <= abs x
             x <= x * x
             x <= x + 1
             0 <= x * x
      negate x <= abs x
negate (abs x) <= x
negate (abs x) <= 0
         abs x <= x * x
negate (abs x) <= negate x

