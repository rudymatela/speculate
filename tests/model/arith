max expr size  =    5
max  #-tests   =  500
min  #-tests   =   25  (to consider p ==> q true)
max  #-vars    =    2  (for inequational and conditional laws)

_ :: Int  (holes: Int)
0 :: Int
1 :: Int
(+) :: Int -> Int -> Int
(*) :: Int -> Int -> Int

      x * 0 == 0
      x * 1 == x
      x + 0 == x
      x * y == y * x
      x + y == y + x
      x + x == x * (1 + 1)
(x * y) * z == x * (y * z)
(x + y) + z == x + (y + z)
  x * y + x == x * (y + 1)

x <= x * x
x <= x + 1
0 <= x * x

