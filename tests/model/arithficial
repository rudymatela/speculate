max expr size  =    5
  |- on ineqs  =    4
  |- on conds  =    4
max  #-tests   =  500
min  #-tests   =   25  (to consider p ==> q true)
max  #-vars    =    2  (for inequational and conditional laws)

_ :: Int  (holes: Int)
0 :: Int
id :: Int -> Int
(+) :: Int -> Int -> Int
f :: Int -> Int -> Int
g :: Int -> Int -> Int
h :: Int -> Int -> Int

       id x == x
      x + 0 == x
      x + y == y + x
      f x y == f x z
      g x 0 == g y z
      h x y == h z y
(x + y) + z == x + (y + z)

y <= g x x
y <= h x 0
y <= f 0 x

