_ :: Int  (holes: Int)
_ :: Bool  (holes: Bool)
0 :: Int
1 :: Int
(+) :: Int -> Int -> Int
abs :: Int -> Int
(<=) :: Int -> Int -> Bool
(<) :: Int -> Int -> Bool
(==) :: Int -> Int -> Bool
False :: Bool
True :: Bool
not :: Bool -> Bool

      abs 0 == 0
      abs 1 == 1
abs (abs x) == abs x
      x + 0 == x
abs (1 + 1) == 1 + 1
      x + y == y + x

0 <= 1
x <= abs x
0 <= abs x

