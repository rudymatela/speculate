_ :: Int  (holes: Int)
_ :: Bool  (holes: Bool)
(+) :: Int -> Int -> Int
abs :: Int -> Int
0 :: Int
1 :: Int
(<=) :: Int -> Int -> Bool
(<) :: Int -> Int -> Bool
(==) :: Int -> Int -> Bool
False :: Bool
True :: Bool
not :: Bool -> Bool
(==) :: Bool -> Bool -> Bool

       abs 0 == 0
       abs 1 == 1
 abs (abs x) == abs x
       x + 0 == x
       x + y == y + x
 abs (1 + 1) == 1 + 1
    (x == 0) == (abs x < 1)
 (x < abs x) == (x < 0)
(x == abs x) == not (x < 0)
 (abs x < x) == False
 (abs x < 0) == False

x <= abs x
0 <= abs x

    x <= 0 ==>   x + abs x == 0
abs x <= y ==> abs (x + y) == x + y
abs y <= x ==> abs (x + y) == x + y
1 == abs x ==> abs (x + 1) == x + abs x

