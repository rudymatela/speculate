_ :: Bool  (holes: Bool)
0 :: Int
1 :: Int
(+) :: Int -> Int -> Int
abs :: Int -> Int
(<=) :: Int -> Int -> Bool
(<) :: Int -> Int -> Bool
(==) :: Int -> Int -> Bool
False :: Bool
True :: Bool
not :: Bool -> Bool

       abs 0 == 0
       abs 1 == 1
 abs (abs x) == abs x
       x + 0 == x
    (x == 0) == (abs x < 1)
     (1 < 0) == False
 (x < abs x) == (x < 0)
(x == abs x) == not (x < 0)
 (abs x < x) == False
 (abs x < 0) == False
 abs (1 + 1) == 1 + 1
       x + y == y + x
     (0 < x) == not (x < 1)

0 <= 1
x <= abs x
0 <= abs x

