_ :: Int  (holes: Int)
_ :: Bool  (holes: Bool)
(+) :: Int -> Int -> Int
id :: Int -> Int
abs :: Int -> Int
0 :: Int
1 :: Int
(<=) :: Int -> Int -> Bool
(<) :: Int -> Int -> Bool
(==) :: Int -> Int -> Bool
not :: Bool -> Bool
True :: Bool
False :: Bool
(==) :: Bool -> Bool -> Bool

        id x == x
 abs (abs x) == abs x
       x + 0 == x
       x + y == y + x
    (x == 0) == (abs x < 1)
 (x < abs x) == (x < 0)
(x == abs x) == not (x < 0)
 (abs x < x) == False
 (abs x < 0) == False

x <= abs x
0 <= abs x

    x <= 0 ==>   x + abs x == 0
abs x <= y ==> abs (x + y) == x + y
abs y <= x ==> abs (x + y) == x + y
1 == abs x ==> abs (x + 1) == x + abs x

