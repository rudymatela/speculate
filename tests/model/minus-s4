_ :: Int  (holes: Int)
0 :: Int
1 :: Int
id :: Int -> Int
negate :: Int -> Int
(+) :: Int -> Int -> Int
(-) :: Int -> Int -> Int

             id x == x
         negate 0 == 0
            x - y == x + negate y
negate (negate x) == x
            x + 0 == x
            x + y == y + x
     x + negate x == 0

       0 <= 1
negate 1 <= 0

