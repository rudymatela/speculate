_ :: Int  (holes: Int)
0 :: Int
1 :: Int
id :: Int -> Int
(+) :: Int -> Int -> Int
(*) :: Int -> Int -> Int

       id x == x
      x * 0 == 0
      x * 1 == x
      x + 0 == x
      x * y == y * x
      x + y == y + x
      x + x == x * (1 + 1)
(x * y) * z == x * (y * z)
(x + y) + z == x + (y + z)
  x * y + x == x * (y + 1)

x <= x * x
x <= x + 1
0 <= x * x

