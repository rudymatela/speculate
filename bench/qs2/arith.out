== Signature ==
     0 :: Int
     1 :: Int
   (+) :: Int -> Int -> Int
   (*) :: Int -> Int -> Int
    id :: Int -> Int
negate :: Int -> Int
   abs :: Int -> Int

== Laws ==
id i = i
abs 0 = 0
abs 1 = 1
negate 0 = 0
i * 0 = 0
i * 1 = i
0 * i = 0
1 * i = i
i + 0 = i
0 + i = i
abs (abs i) = abs i
abs (negate i) = abs i
negate (negate i) = i
i * j = j * i
i + j = j + i
negate i * j = i * negate j
i + negate i = 0
abs (i * i) = i * i
negate (i * j) = i * negate j
abs (1 + 1) = 1 + 1
(i * j) * k = i * (j * k)
(i + j) + k = i + (j + k)
i * (j + 1) = i + (i * j)
abs i * abs j = abs (i * j)
abs i + abs i = abs (i + i)
negate i + negate j = negate (i + j)
abs (i + abs i) = i + abs i
abs (1 + abs i) = 1 + abs i
i * (j + j) = j * (i + i)

